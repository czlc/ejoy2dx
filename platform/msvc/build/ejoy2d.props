<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <EJ2DX_PRJ_ROOT>$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), ejoy2d.root))</EJ2DX_PRJ_ROOT>
    <EJ2DX_INCLUDE>$(EJ2DX_PRJ_ROOT)\include</EJ2DX_INCLUDE>
    <EJ2DX_GLEW>$(EJ2DX_PRJ_ROOT)\src\glew</EJ2DX_GLEW>
    <EJ2DX_MAIN>$(EJ2DX_PRJ_ROOT)\src\main</EJ2DX_MAIN>
    <EJ2DX_ZLIB>$(EJ2DX_PRJ_ROOT)\src\zlib</EJ2DX_ZLIB>
    <EJ2DX_ROOT>$(EJ2DX_PRJ_ROOT)\..\..\src\clib</EJ2DX_ROOT>
    <EJ2DX_FRAMEWORK>$(EJ2DX_ROOT)\framework</EJ2DX_FRAMEWORK>
    <EJ2DX_PLATFORM>$(EJ2DX_ROOT)\platform</EJ2DX_PLATFORM>
    <EJ2DX_UTLS>$(EJ2DX_ROOT)\utls</EJ2DX_UTLS>
    <EJ2D_ROOT>$(EJ2DX_PRJ_ROOT)\..\..\ejoy2d</EJ2D_ROOT>
    <EJ2D_LUA>$(EJ2D_ROOT)\lua</EJ2D_LUA>
    <EJOY2DX_EXE>$(SolutionDir)..\output\$(Configuration)\</EJOY2DX_EXE>
    <EJOY2DX_OBJ>$(SolutionDir)..\obj\$(Configuration)\$(ProjectName)</EJOY2DX_OBJ>
    <EJOY2DX_LIB>$(SolutionDir)..\lib\$(Configuration)\</EJOY2DX_LIB>
    <EJOY2DX_COMMON_LIB>$(EJ2DX_PRJ_ROOT)\common_lib</EJOY2DX_COMMON_LIB>
  </PropertyGroup>
  <PropertyGroup>
    <IntDir>$(EJOY2D_OBJ)</IntDir>
    <OutDir>$(EJOY2D_LIB)</OutDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <WarningLevel>Level3</WarningLevel>
    </ClCompile>
    <Link />
    <ResourceCompile>
      <IgnoreStandardIncludePath>
      </IgnoreStandardIncludePath>
    </ResourceCompile>
    <ResourceCompile>
      <ShowProgress>
      </ShowProgress>
    </ResourceCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="EJOY2D">
      <Value>$(EJOY2D)</Value>
    </BuildMacro>
    <BuildMacro Include="EJOY2DX_EXE">
      <Value>$(EJOY2DX_EXE)</Value>
    </BuildMacro>
    <BuildMacro Include="EJOY2D_ROOT">
      <Value>$(EJOY2D_ROOT)</Value>
    </BuildMacro>
    <BuildMacro Include="EJOY2D_OBJ">
      <Value>$(EJOY2D_OBJ)</Value>
    </BuildMacro>
    <BuildMacro Include="EJOY2D_LIB">
      <Value>$(EJOY2D_LIB)</Value>
    </BuildMacro>
    <BuildMacro Include="EJOY2D_LUA">
      <Value>$(EJOY2D_LUA)</Value>
    </BuildMacro>
    <BuildMacro Include="EJOY2D_GLEW">
      <Value>$(EJOY2D_GLEW)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>
